Ts <- c("t=10", "t=20", "t=30", "t=40", "t=50"); #
#
# Data collection and aggregation#
dta = data.frame()#
for(a in Ts){#
	data1 = read.csv(paste(a, ".csv", sep=""),header=TRUE)#
    data1$T = a#
    dta = rbind(dta, data1)#
}#
dta$T = factor(dta$T, levels=Ts)#
#
p <- ggplot(aes_string(x="f1.x.", y="f2.x.", color="T"), data=dta) + geom_point() + ggtitle("Tuning parameter T on bi-objective problem ZDT3") + xlim(0, 1) + ylim(-1, 1) +  xlab("function objective f1(x)") + ylab("function objective f2(x)")
